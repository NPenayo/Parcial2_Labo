#ifndef EDITORIAL_H_
#define EDITORIAL_H_

typedef struct{
	int _id;
	char _description[128];
}Editorial;

Editorial getEditorialById(Editorial* edList, int);
Editorial getEditorialByDescription(Editorial* edList, char*);
int setEditorialId(Editorial, int);
int setEditorialDescription(Editorial, char*);
#endif /* EDITORIAL_H_ */
