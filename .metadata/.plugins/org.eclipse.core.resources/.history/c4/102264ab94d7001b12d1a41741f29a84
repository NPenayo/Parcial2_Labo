#ifndef EDITORIAL_H_
#define EDITORIAL_H_

typedef struct{
	int _id;
	char _description[128];
}Editorial;

int getEditorialId(Editorial);
char getEditorialDescription(int);
int setEditorialId(Editorial);
int setEditorialDescription(Editorial);
#endif /* EDITORIAL_H_ */
